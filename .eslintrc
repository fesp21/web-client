{
  "extends": "eslint-config-airbnb",
  "rules": {
    "no-amd": 0,
    "react/jsx-file-extension": 0
  },
  "plugins": [
    "react", "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": ["node_modules", "src"]
    }
  },
  "globals": {
  }
}
